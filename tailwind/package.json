{
  "name": "django-unfold",
  "description": "Modern Django admin theme for seamless interface development",
  "scripts": {
    "tw-unfold:build": "./sync-unfold.sh && tailwindcss -i ./unfold/custom.css -o ../docrepo/staticfiles/unfold/css/styles.css --minify",
    "tw-unfold:watch": "./sync-unfold.sh && tailwindcss -i ./unfold/custom.css -o ../docrepo/staticfiles/unfold/css/styles.css --watch --minify",
    "tw-doc:build": "tailwindcss -c tailwind-doc.config.js  -i docrepo/custom.css -o ../docrepo/staticfiles/tailwind/styles.css --minify",
    "tw-doc:watch": "tailwindcss -c tailwind-doc.config.js -i docrepo/custom.css -o ../docrepo/staticfiles/tailwind/styles.css --watch"
  },
  "dependencies": {
    "tailwindcss": "^3.4.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10"
  }
}
